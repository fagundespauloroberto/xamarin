		<Label x:Name="Header" Text="SwitchCell" FontSize="50" FontAttributes="Bold" HorizontalOptions="Center" />
		<TableView Intent="Form">
			<TableRoot>
				<TableSection>
					<SwitchCell Text="SwitchCell:" />
				</TableSection>
			</TableRoot>
		</TableView>
	</StackLayout>
</ContentPage>